# API Keys and Configuration Template
# Copy this file to .env and fill in your actual API keys

# AI4Thai API key for tokenization, POS tagging, and G2P
VITE_AI4THAI_API_KEY=your_ai4thai_api_key_here

# Airtable Configuration
VITE_AIRTABLE_API_TOKEN=your_airtable_api_token_here
VITE_AIRTABLE_BASE_ID=your_airtable_base_id_here
VITE_AIRTABLE_TABLE_NAME=LookUp

# OpenAI API key (optional - can also be set via window.SMARTSUBS_GPT_KEY)
VITE_OPENAI_API_KEY=your_openai_api_key_here
